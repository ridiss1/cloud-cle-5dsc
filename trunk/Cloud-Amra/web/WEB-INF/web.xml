<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AuthentificationServlet</servlet-name>
        <servlet-class>PAAS.Servlets.AuthentificationServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ContactStudentServlet</servlet-name>
        <servlet-class>PAAS.Servlets.ContactStudentServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ProfilStudentServlet</servlet-name>
        <servlet-class>PAAS.Servlets.ProfilStudentServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>VmStudentServlet</servlet-name>
        <servlet-class>PAAS.Servlets.VmStudentServlet</servlet-class>
    </servlet>
    
    
    
    
    <servlet-mapping>
        <servlet-name>AuthentificationServlet</servlet-name>
        <url-pattern>/accueil</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ProfilStudentServlet</servlet-name>
        <url-pattern>/profil</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ContactStudentServlet</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>VmStudentServlet</servlet-name>
        <url-pattern>/vm</url-pattern>
    </servlet-mapping>
</web-app>
